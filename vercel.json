{
  "version": 2,
  "name": "Physio",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api": {
      "handler": "src/index.ts",
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    { "src": "/api/*", "dest": "src/index.ts" },
    { "src": "/(.*)", "dest": "src/index.ts" }
  ]
}
